{{#if heading}}
<h3 id="{{headingId}}">Inhoudsopgave</h3>
{{/if}}
<ul data-decorator="tree" role="tree" aria-labelledby="{{headingId}}" class="treeview2 {{ modifier }}" id="treeview-{{ id }}">
  <li role="treeitem" aria-level="1" aria-setsize="2" aria-posinset="1" aria-expanded="false">
    <span class="foldable">
      Hoofdstuk 1
      <small>Ministerie van Binnenlandse Zaken en Koninkrijksrelaties</small>
    </span>

    <ul role="group">
      <li role="none">
        <a role="treeitem" aria-level="2" aria-setsize="3" aria-posinset="1" href="#">Artikel 1</a>
      </li>
      <li role="treeitem" aria-level="2" aria-setsize="3" aria-posinset="2" aria-expanded="false">
        <span>Artikel 2</span>
        <ul role="group">
          <li role="none"><a href="#" role="treeitem" aria-level="3" aria-setsize="3" aria-posinset="1">Artikel 2</a></li>
          <li role="none"><a href="#" role="treeitem" aria-level="3" aria-setsize="3" aria-posinset="2">Artikel 2.1</a> </li>
          <li role="treeitem" aria-level="3" aria-setsize="3" aria-posinset="3" aria-expanded="false">
            <span>Artikel 2.2</span>
            <ul role="group">
              <li role="none"><a href="#" role="treeitem" aria-level="4" aria-setsize="2" aria-posinset="1">Artikel 2.2</a></li>
              <li role="treeitem" aria-level="4" aria-setsize="2" aria-posinset="2" aria-expanded="false">
                <span>Artikel 2.2.1</span>
                <ul role="group">
                  <li role="none"><a href="#" role="treeitem" aria-level="5" aria-setsize="2" aria-posinset="1">Artikel 2.2.1</a></li>
                  <li role="none"><a href="#" role="treeitem" aria-level="5" aria-setsize="2" aria-posinset="2">Artikel 2.2.2</a></li>
                </ul>
              </li>
            </ul>
          </li>
        </ul>
      </li>
      <li role="none"><a role="treeitem" aria-level="2" aria-setsize="3" aria-posinset="3" href="#">Artikel 3</a>
    </ul>
  </li>
  <li role="none">
    <a href="#" role="treeitem" aria-level="1" aria-setsize="2" aria-posinset="2">
      Hoofdstuk 2
      <small>Ministerie van Defensie</small>
      <span>(Artikelen 1.1-1.23)</span>
    </a>
  </li>
</ul>
