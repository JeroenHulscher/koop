<table class="{{ modifier }}">
<caption>Deze tabel bevat teamleden van KOOP Overheid.nl</caption>
<thead>
  {{#each items }}
  {{#if @first}}
  <tr>
    {{#each item }}
    <th scope="col">{{ type }}</th>
    {{/each}}
  </tr>
  {{/if}}
  {{/each }}
</thead>
<tbody>
  {{#each items }}
  <tr>
    {{#each item }}
      {{#if cellType }}
      <td data-before="{{ type }}" class="status {{ cellType }}"><span>{{ value }}</span></td>
      {{ else }}
       <td data-before="{{ type }}">{{ value }}</td>
      {{/if }}
    {{/each }}
  </tr>
  {{/each }}
</tbody>
</table>
