<div data-decorator="init-passwordstrength" class="{{modifier}}">
  <div class="form__row">
    {{render "@input-text" newPWInput }}
  </div>
  <div class="well js-passwordstrength__regexcontainer {{modifierRegexContainer}}">
    <div>
      <ul class="list list--checked">
        {{#each regexOptions}}
        <li data-regex="{{regex}}" data-regex-label="">{{label}}</li>
        {{/each}}
      </ul>
    </div>
  </div>
  {{#if duplicateregexcontainer}}
    <div class="well js-passwordstrength__duplicateregexcontainer {{modifierRegexDuplicateContainer}}"></div>
  {{/if}}
</div>




