{% for item in items %}
<div class="form__selectedfilter {% if item.modifier %}form__selectedfilter--{{ items.modifier }}{% endif %}">
  {{item.label}}{% if item.value and item.label %} <span>{{ item.value }}</span>{% else %}{{ item.value}}{% endif %}
  <a href="{{ item.url ?: '#' }}" class="form__selectedfilter__remove">
    <span class="visually-hidden">verwijder</span>
  </a>
</div>
{% endfor %}
