<header class="header">
  <nav class="header__nav header__nav--closed" id="nav">
    <h2 class="visually-hidden">Primaire navigatie</h2>
    <div class="container">
      <ul class="header__primary-nav list list--unstyled">
        <li class="hidden-mobile"><a href="#">Verlaat KPM</a></li>
      </ul>
    </div>
  </nav>
</header>
<div role="main">
  <div class="container">
    <div class="container">
      <h1>Zoek een locatie</h1>
      <p>Vul een term in om een locatie te zoeken.</p>
      <div class="columns">
        <div class="form__row--large">
          {{ render "@input-autocomplete" inputAutocompleteContext }}
        </div>
        <div>
        {{render "@button-icon" button1Context}}
        {{render "@button-icon" button2Context}}
        {{render "@button-icon" button3Context}}
        </div>
      </div>
    </div>
  </div>
  <div class="container columns-map k-future" data-decorator="handle-collapse" data-config='{ "trigger": "sidebar-trigger" }'>
    <div class="sidebar-column">
      <div class="sidebar-column-content" data-decorator="active-item-toggle">
        <div class="container">
          <div class="container">
            <h2 class="h2 h2--large">Gekozen locaties</h2>
            <div class="content-item collapsed" data-decorator="handle-collapse" data-config='{ "trigger": "table-one-trigger" }'>
              <h3>Gemeente 's-Gravenhage</h3>
              <table class="table table--small">
                <tbody>
                  <tr class="type-row">
                    <th>Type</th>
                    <td>Gemeente</td>
                  </tr>
                  <tr class="source-row">
                    <th>Provinciecode</th>
                    <td>PV28</td>
                  </tr>
                  <tr class="detail-row">
                    <th>Provincienaam</th>
                    <td>Zuid-Holland</td>
                  </tr>
                  <tr class="detail-row">
                    <th>Gemeentecode</th>
                    <td>0518</td>
                  </tr>
                  <tr class="detail-row">
                    <th>Gemeentenaam</th>
                    <td>'s-Gravenhage</td>
                  </tr>
                  <tr class="detail-row">
                    <th>ID</th>
                    <td>gem-339aeade1bbb0005ba845b14d29d2c15</td>
                  </tr>
                </tbody>
              </table>
              {{render "@button-icon" table1ExpandButtonContext }}
              {{render "@button-icon" button5Context }}
              {{render "@button-icon" button6Context }}
            </div>

            <div class="content-item collapsed" data-decorator="handle-collapse" data-config='{ "trigger": "table-two-trigger" }'>
              <h3>Voorburg</h3>
              <table class="table table--small">
                <tbody>
                  <tr class="type-row">
                    <th>Type</th>
                    <td>Gemeente</td>
                  </tr>
                  <tr class="source-row">
                    <th>Bron</th>
                    <td>Bestuurlijke Grenzen</td>
                  </tr>
                  <tr class="detail-row">
                    <th>Bron</th>
                    <td>Bestuurlijke Grenzen</td>
                  </tr>
                  <tr class="detail-row">
                    <th>Bron</th>
                    <td>Bestuurlijke Grenzen</td>
                  </tr>
                </tbody>
              </table>
              {{render "@button-icon" table2ExpandButtonContext }}
              {{render "@button-icon" button5Context }}
              {{render "@button-icon" button6Context }}
            </div>

            <div class="content-item collapsed" data-decorator="handle-collapse" data-config='{ "trigger": "table-three-trigger" }'>
              <h3>Rijswijk</h3>
              <table class="table table--small">
                <tbody>
                  <tr class="type-row">
                    <th>Type</th>
                    <td>Gemeente</td>
                  </tr>
                  <tr class="source-row">
                    <th>Bron</th>
                    <td>Bestuurlijke Grenzen</td>
                  </tr>
                  <tr class="detail-row">
                    <th>Bron</th>
                    <td>Bestuurlijke Grenzen</td>
                  </tr>
                  <tr class="detail-row">
                    <th>Bron</th>
                    <td>Bestuurlijke Grenzen</td>
                  </tr>
                  <tr class="detail-row">
                    <th>Bron</th>
                    <td>Bestuurlijke Grenzen</td>
                  </tr>
                </tbody>
              </table>
              {{render "@button-icon" table3ExpandButtonContext }}
              {{render "@button-icon" button5Context }}
              {{render "@button-icon" button6Context }}
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="main-column" style="background: #ccc">
      <a href="#" class="trigger-collapse sidebar-trigger"><span class="visually-hidden">In- of uitklappen</span> </a>
      <iframe width="100%" height="800px" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://www.openstreetmap.org/export/embed.html?bbox=4.044342041015626%2C51.97155731422007%2C4.535293579101563%2C52.13959907316872&amp;layer=mapnik" style="border: 1px solid black"></iframe><br/><small><a href="https://www.openstreetmap.org/#map=12/52.0557/4.2898">View Larger Map</a></small>
    </div>
  </div>
</div>
{{ render "@footer" }}