      <h3 class="section-chapter__subheading">Vaste positie (top)</h3>
      <div class="ds-preview">
        <div class="ds-preview__demo">
          {{render "@alert--fixed-to-top" alertFixed merge=true }}
          <div id="holder_1"></div>
          <script>
            document.getElementsByClassName('alert--fixed')[0].classList.remove('show');
            var link = document.createElement('a');
            link.href = '#';
            link.innerHTML = 'Toon alert';
            link.id = 'linkId1';
            document.getElementById('holder_1').appendChild(link);
            document.getElementById('linkId1').addEventListener("click", function (e) {
              document.getElementsByClassName('alert--fixed')[0].classList.add('show');
              e.preventDefault();
            }, false);
          </script>
        </div>
        <div class="ds-preview__content">
          <p>
            <a href="https://jeroenhulscher.nl/koop/components/detail/alert--fixed-to-top.html">Bekijk component 'Alert vaste positie (top)'</a>
