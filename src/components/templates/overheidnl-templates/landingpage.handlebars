{{ render "@header" headerContext merge="true" }}

<div class="row row--page-opener">
  <div class="container">
    {{ render "@breadcrumb" }}
  </div>
</div>

{{#each rowLanding }}
<div class="row row--image row--landing" style="background-image: url({{ image }});">
  <div class="container">
    <div class="content--onimage">
      <h1>{{ title }}</h1>
      <p class="intro--text">{{ intro }}</p>
    </div>
  </div>
</div>
{{/each}}

<div class="row">
  <div class="container">
  {{#each linkSetGroup }}
    <div class="columns">
      {{#each linkSet }}
      <div>
        <h2>{{ title }}</h2>
        {{#if links }}
        <ul class="list--linked">
        {{#each links }}
          <li><a href="{{ url }}">{{ text }}</a></li>
        {{/each}}
        </ul>
        {{/if}}
      </div>
      {{/each}}
    </div>
  {{/each}}
  </div>
</div>

{{ render "@footer" }}
