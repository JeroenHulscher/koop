    "vrtest_stopxml-templates": "backstop reference --configPath=backstop_stopxml-templates.json && backstop test --configPath=backstop_stopxml-templates.json",
    "atest": "pa11y-ci --threshold 10 --config test/pa11yci.json"
  },
  "repository": {
    "type": "git",
